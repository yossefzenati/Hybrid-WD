! inlist1 for the primary in a binary model

&star_job

  !Load model
    !load_saved_model = .true.
    !saved_model_name = 'Model_4M.mod'

  !Save a model when the run is done
      !save_model_when_terminate = .true.
      !save_model_filename = 'Final_4M.mod'

    !new_v_flag = .true.
  ! Write a profile to a given name when the run is done
    !write_profile_when_terminate = .true.
    !filename_for_profile_when_terminate = 'profile_end3.data'
    !change_initial_net = .true.      
    !new_net_name = 'o18_and_ne22.net'

    !set_initial_age = .true.
    !initial_age = 0
    !set_tau_factor = .true.
    !set_to_this_tau_factor = 50
    !pgstar_flag = .true.

/ ! end of star_job namelist


&controls

  ! The mass is in the inlist_project file along with the period

  ! Output rate
  !write_header_frequency = 10
